{
  "name": "@etco-job-application/entry-mobile",
  "version": "0.0.0",
  "main": "__generated__/AppEntry.js",
  "private": true,
  "scripts": {
    "checks": "yarn typecheck && yarn lint",
    "typecheck": "../../node_modules/.bin/tsc -p . --noEmit",
    "lint": "../../node_modules/.bin/eslint '**/*.ts{,x}'",
    "lint:fix": "../../node_modules/.bin/eslint --fix '**/*.ts{,x}'",
    "start": "yarn generate-app-json && expo start",
    "android": "expo start --android",
    "android-device-reverse": "for port in 19000 19001 19002; do adb reverse tcp:$port tcp:$port; done",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "postinstall": "expo-yarn-workspaces postinstall",
    "publish": "yarn generate-app-json && expo publish --release-channel $npm_config_release_channel",
    "build:ios": "yarn generate-app-json && expo build:ios --type archive --release-channel $npm_config_release_channel",
    "build:android": "yarn generate-app-json && expo build:android --type apk --release-channel $npm_config_release_channel",
    "download-build:ios": "mkdir -p .build && wget \"$(expo url:ipa)\" -O .build/app-$npm_config_release_channel.ipa",
    "download-build:android": "mkdir -p .build && wget \"$(expo url:apk)\" -O .build/app.$npm_config_release_channel.apk",
    "generate-app-json": "../../node_modules/.bin/ts-node ./scripts/prepare-app-json.ts"
  },
  "dependencies": {
    "@etco-job-application/core": "0.0.0",
    "axios": "^0.21.1",
    "expo": "~37.0.3",
    "expo-constants": "~9.0.0",
    "formik": "^2.1.4",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-screens": "~2.2.0",
    "react-native-svg": "11.0.1",
    "react-native-svg-animated-linear-gradient": "^0.3.4",
    "react-native-web": "~0.11.7",
    "yup": "^0.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@types/react": "~16.9.23",
    "@types/react-native": "~0.61.17",
    "babel-preset-expo": "~8.1.0",
    "expo-yarn-workspaces": "^1.2.1",
    "typescript": "~3.8.3"
  }
}
